{
	"cSpell.ignorePaths": [
		"package.json",
		"package-lock.json",
		"yarn.lock",
		"pnpm-lock.yaml",
		"node_modules",
		"vscode-extension",
		".git/objects",
		".vscode",
		".vscode-insiders",
		"CHANGELOG.md",
		"dist",
		"public",
		"styles"
	],
	"cSpell.words": [
		"AMAP",
		"antdesign",
		"antv",
		"apacheecharts",
		"areaspline",
		"bmapgl",
		"colord",
		"echarts",
		"gitee",
		"gridicons",
		"iconify",
		"jsapi",
		"naiveui",
		"Popconfirm",
		"Posva",
		"Shenzhen",
		"Sider",
		"tauri",
		"unocss",
		"unplugin",
		"vditor",
		"VERCEL",
		"Vite",
		"vitejs",
		"vuedraggable",
		"vueuse",
		"wangeditor",
		"wechat",
		"xgplayer",
		"yanbowe",
		"ភាសាខ្មែរ"
	],
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.fontLigatures": true,
	"editor.formatOnSave": false,
	"editor.quickSuggestions": {
		"strings": true
	},
	"editor.tabSize": 2,
	"eslint.validate": ["json"],
	"files.associations": {
		"*.env.*": "dotenv",
		"*.svg": "html"
	},
	"files.eol": "\n",
	"i18n-ally.displayLanguage": "zh-CN",
	"i18n-ally.enabledParsers": ["ts"],
	"i18n-ally.enabledFrameworks": ["vue"],
	"i18n-ally.editor.preferEditor": true,
	"i18n-ally.keystyle": "nested",
	"i18n-ally.localesPaths": ["src/locales/lang"],
	"material-icon-theme.activeIconPack": "vue",
	"[html][css][less][scss][sass][markdown][yaml][yml][jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true
	},
	"prettier": {},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/bower_components": true,
		"**/node_modules": true,
		"**/tmp": true,
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true,
		".github": true,
		".vscode": true,
		"docker": true,
		"mock": true,
		"patches": true,
		"CHANGELOG.md": true,
		"scripts": true,
		"build": true,
		"src/components": true,
		"src/assets": true,
		"src/composables": true,
		"src/config": true,
		"src/constants": true,
		"src/context": true,
		"src/directives": true,
		"src/layouts": true,
		"src/hooks": true,
		"src/locales": true,
		"src/service": true,
		"src/settings": true,
		"src/styles": true,
		"src/utils": true,
		"src/typings": true,
		"src/store": true,
		"pnpm-lock.yaml": true,
		".editorconfig": true,
		".eslintignore": true,
		"src/router/guard": true,
		"src/views/auth-demo": true,
		"src/views/component": true,
		"src/views/document": true,
		"src/views/_builtin": true,
		"public": true,
		"src/views/exception": true,
		"src/views/function": true,
		"src/views/management": true,
		"src/views/multi-menu": true,
		"src/views/plugin": true,
		".eslintrc.js": true,
		".gitattributes": true,
		".npmrc": true,
		"LICENSE": true,
		"Makefile": true,
		"README.md": true,
		"uno.config.ts": true
	},
	"hide-files.files": [
		".github",
		".vscode",
		"docker",
		"mock",
		"patches",
		"CHANGELOG.md",
		"scripts",
		"build",
